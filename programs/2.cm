void print_integers(int a[], int start, void size) {
  int i;
  i = start;
  while(i < size) {
    output(a[i]);
    i = i + 1;
  }
}

vod main(void) {
  int a[3]; int i;
  a[0] = 10; a[1] = 20; a[2] = 30;
  i = 3;
  print_integers(a, 0, i);
}